<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:gravity="center"
    android:orientation="vertical"
    android:paddingBottom="5dp"
    android:paddingLeft="5dp"
    android:paddingRight="5dp"
    android:paddingTop="5dp" >

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scrollbars="none" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/linearLayout1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal" >

                <LinearLayout
                    android:id="@+id/lyaddnew_hor"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="30dp"
                    android:background="@color/transprant_white"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:id="@+id/ly_add_new"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >

                        <com.ignite.custom.view.CustomTextView
                            android:id="@+id/txt_co_title"
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            android:layout_gravity="center"
                            android:background="@color/green"
                            android:gravity="center_vertical"
                            android:paddingLeft="25dp"
                            android:text="အသစ္ထည့္ျခင္း"
                            android:textColor="@color/white"
                            android:textSize="20dp" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:padding="20dp" >

                            <com.ignite.custom.view.CustomButton
                                android:id="@+id/btnBarcode"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="10dp"
                                android:background="@drawable/bg_button"
                                android:gravity="left|center_vertical"
                                android:paddingLeft="20dp"
                                android:text="ပစၥည္းအသစ္ထည့္ရန္"
                                android:textColor="@color/white"
                                android:textSize="18dp" />

                            <com.ignite.custom.view.CustomButton
                                android:id="@+id/btn_new_purchase_voucher"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="10dp"
                                android:background="@drawable/bg_button"
                                android:gravity="left|center_vertical"
                                android:paddingBottom="5dp"
                                android:paddingLeft="20dp"
                                android:paddingTop="5dp"
                                android:text="အ၀ယ္ေဘာင္ ခ်ာအသစ္ ထည့္ရန္"
                                android:textColor="@color/white"
                                android:textSize="18dp" />

                            <View
                                android:id="@+id/View03"
                                android:layout_width="match_parent"
                                android:layout_height="3dp"
                                android:layout_marginBottom="10dp"
                                android:background="@color/gray"
                                android:visibility="visible" />

                            <com.ignite.custom.view.CustomButton
                                android:id="@+id/btn_sale_return"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/bg_button"
                                android:gravity="left|center_vertical"
                                android:paddingBottom="5dp"
                                android:paddingLeft="20dp"
                                android:paddingTop="5dp"
                                android:text="ျပန္၀င္ ပစၥည္း ေဘာင္ ခ်ာအသစ္ထည့္ရန္"
                                android:textColor="@color/white"
                                android:textSize="18dp"
                                android:visibility="visible" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/lyupdate_hor"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="30dp"
                    android:background="@color/transprant_white"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:id="@+id/ly_update"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >

                        <com.ignite.custom.view.CustomTextView
                            android:id="@+id/txt_update"
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            android:background="@color/green"
                            android:gravity="center_vertical"
                            android:paddingLeft="25dp"
                            android:text="ျပင္ ဆင္ ျခင္း"
                            android:textColor="@color/white"
                            android:textSize="20dp" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:padding="20dp" >

                            <com.ignite.custom.view.CustomButton
                                android:id="@+id/btn_update_category"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="10dp"
                                android:background="@drawable/bg_button"
                                android:gravity="left|center_vertical"
                                android:paddingLeft="20dp"
                                android:text="ပစၥည္းအမ်ိဳးအစား ျပင္ ရန္"
                                android:textColor="@color/white"
                                android:textSize="18dp" />

                            <com.ignite.custom.view.CustomButton
                                android:id="@+id/btn_update_subcategory"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="10dp"
                                android:background="@drawable/bg_button"
                                android:gravity="left|center_vertical"
                                android:paddingLeft="20dp"
                                android:text="ပစၥည္းအမ်ိဳးအစားခြဲ ျပင္ ရန္"
                                android:textColor="@color/white"
                                android:textSize="18dp" />

                            <com.ignite.custom.view.CustomButton
                                android:id="@+id/btn_update_item"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="10dp"
                                android:background="@drawable/bg_button"
                                android:gravity="left|center_vertical"
                                android:paddingLeft="20dp"
                                android:text="ပစၥည္းအမည္ ျပင္ ရန္"
                                android:textColor="@color/white"
                                android:textSize="18dp" />

                            <com.ignite.custom.view.CustomButton
                                android:id="@+id/btn_update_prices"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/bg_button"
                                android:gravity="left|center_vertical"
                                android:paddingLeft="20dp"
                                android:text="ေရာင္းေစ်း ျပင္ ရန္"
                                android:textColor="@color/white"
                                android:textSize="18dp" />

                            <com.ignite.custom.view.CustomButton
                                android:id="@+id/btn_admin_acc"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="10dp"
                                android:background="@drawable/bg_button"
                                android:gravity="left|center_vertical"
                                android:paddingLeft="20dp"
                                android:text="Update Admin Account"
                                android:textColor="@color/white"
                                android:textSize="18dp"
                                android:visibility="gone" />

                            <com.ignite.custom.view.CustomButton
                                android:id="@+id/btn_update_cus"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="10dp"
                                android:background="@drawable/bg_button"
                                android:gravity="left|center_vertical"
                                android:paddingLeft="20dp"
                                android:text="Update Customer Info"
                                android:textColor="@color/white"
                                android:visibility="gone" />

                            <com.ignite.custom.view.CustomButton
                                android:id="@+id/btn_update_sup"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/bg_button"
                                android:gravity="left|center_vertical"
                                android:paddingLeft="20dp"
                                android:text="Update Supplier Info"
                                android:textColor="@color/white"
                                android:textSize="18dp"
                                android:visibility="gone" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/lyreport_hor"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:background="@color/transprant_white"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:id="@+id/ly_report"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >

                        <com.ignite.custom.view.CustomTextView
                            android:id="@+id/txt_report"
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            android:background="@color/green"
                            android:gravity="center_vertical"
                            android:paddingLeft="25dp"
                            android:text="အစီရင္ ခံစာႏွင့္ လူအသစ္ထည့္ျခင္း"
                            android:textColor="@color/white"
                            android:textSize="20dp" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:padding="20dp" >

                            <com.ignite.custom.view.CustomButton
                                android:id="@+id/btn_sales_report"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="10dp"
                                android:background="@drawable/bg_button"
                                android:gravity="left|center_vertical"
                                android:paddingLeft="20dp"
                                android:text="အ ေရာင္းမွတ္တမ္း"
                                android:textColor="@color/white"
                                android:textSize="18dp" />

                            <com.ignite.custom.view.CustomButton
                                android:id="@+id/btn_purchase_report"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="10dp"
                                android:background="@drawable/bg_button"
                                android:gravity="left|center_vertical"
                                android:paddingLeft="20dp"
                                android:text="အ၀ယ္မွတ္တမ္း"
                                android:textColor="@color/white"
                                android:textSize="18dp" />

                            <com.ignite.custom.view.CustomButton
                                android:id="@+id/btn_sale_return_report"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="10dp"
                                android:background="@drawable/bg_button"
                                android:gravity="left|center_vertical"
                                android:paddingLeft="20dp"
                                android:text="Sale Return Report"
                                android:textColor="@color/white"
                                android:textSize="18dp"
                                android:visibility="gone" />

                            <com.ignite.custom.view.CustomButton
                                android:id="@+id/btn_other_reports"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="10dp"
                                android:background="@drawable/bg_button"
                                android:gravity="left|center_vertical"
                                android:paddingLeft="20dp"
                                android:text="အျခားမွတ္တမ္းမ်ား"
                                android:textColor="@color/white"
                                android:textSize="18dp" />

                            <View
                                android:id="@+id/view1"
                                android:layout_width="match_parent"
                                android:layout_height="3dp"
                                android:layout_marginBottom="10dp"
                                android:background="@color/gray" />

                            <com.ignite.custom.view.CustomButton
                                android:id="@+id/btn_add_admin_account"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="10dp"
                                android:background="@drawable/bg_button"
                                android:gravity="left|center_vertical"
                                android:paddingLeft="20dp"
                                android:text="စီမံသူ [အသစ္/ျပင္/ဖ်က္] ရန္"
                                android:textColor="@color/white"
                                android:textSize="16dp" />

                            <com.ignite.custom.view.CustomButton
                                android:id="@+id/btn_sale_person"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="10dp"
                                android:background="@drawable/bg_button"
                                android:gravity="left|center_vertical"
                                android:paddingLeft="20dp"
                                android:text="ေရာင္းသူ [အသစ္/ျပင္/ဖ်က္] ရန္"
                                android:textColor="@color/white"
                                android:textSize="16dp" />

                            <com.ignite.custom.view.CustomButton
                                android:id="@+id/btn_customer_info"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="10dp"
                                android:background="@drawable/bg_button"
                                android:gravity="left|center_vertical"
                                android:paddingLeft="20dp"
                                android:text="Add New Customer Info"
                                android:textColor="@color/white"
                                android:visibility="gone" />

                            <com.ignite.custom.view.CustomButton
                                android:id="@+id/btn_supplier_info"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="10dp"
                                android:background="@drawable/bg_button"
                                android:gravity="left|center_vertical"
                                android:paddingLeft="20dp"
                                android:text="လကၠားဆုိင္ [အသစ္/ျပင္/ဖ်က္] ရန္"
                                android:textColor="@color/white"
                                android:textSize="16dp" />

                            <com.ignite.custom.view.CustomButton
                                android:id="@+id/btn_register_notification"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/bg_button"
                                android:gravity="left|center_vertical"
                                android:paddingLeft="20dp"
                                android:text="လက္က်န္ သတိေပး [အသစ္/ျပင္/ဖ်က္] ရန္"
                                android:textColor="@color/white"
                                android:textSize="14dp" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
    </ScrollView>

</RelativeLayout>